<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>A way to write Fss in Feliz.</Description>
    <RepositoryUrl>https://github.com/Bjorn-Strom/Fss.git</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Bjorn-Strom/Fss</PackageProjectUrl>
    <PackageTags>fsharp;emotion;CSS;styling;css-in-lang;fable;react;html;feliz</PackageTags>
    <Authors>Bjørn-Ivar Strøm</Authors>
    <Version>1.0.0</Version>
    <PackageLicenseUrl>https://github.com/Bjorn-Strom/Fss/blob/master/LICENSE.md</PackageLicenseUrl>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputType>Library</OutputType>
    <AssemblyName>Fss-lib-feliz</AssemblyName>
    <PackageReleaseNotes>Add basic Fss syntax to Feliz.</PackageReleaseNotes>
    <RootNamespace>FssFeliz</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <NpmDependencies>
      <NpmPackage Name="@emotion/css" Version="&gt;= 11.0.0" />
    </NpmDependencies>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="FssFeliz.fs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\fss\Fss.Engine.fsproj" />
  </ItemGroup>
    <ItemGroup>
        <Content Include="*.fsproj; *.fs; *.js;" Exclude="**\*.fs.js" PackagePath="fable\" />
    </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>