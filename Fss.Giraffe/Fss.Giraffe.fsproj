<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Description>Provides helper functions for using Fss with Giraffe View Engine.</Description>
        <RepositoryUrl>https://github.com/Bjorn-Strom/Fss.git</RepositoryUrl>
        <PackageProjectUrl>https://github.com/Bjorn-Strom/Fss</PackageProjectUrl>
        <PackageTags>fsharp;fss;fss.core;CSS;styling;css-in-lang;giraffe;viewengine;html</PackageTags>
        <Authors>Bjørn-Ivar Strøm</Authors>
        <Version>2.2.0</Version>
        <PackageLicenseUrl>https://github.com/Bjorn-Strom/Fss/blob/master/LICENSE.md</PackageLicenseUrl>
        <TargetFramework>netstandard2.0</TargetFramework>
        <OutputType>Library</OutputType>
        <PackageReleaseNotes>Add zero length unit to more easily work with 0 length units.</PackageReleaseNotes>
        <RootNamespace>Fss.Fable</RootNamespace>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageId>Fss-lib.Giraffe</PackageId>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="FssGiraffe.fs" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Giraffe.ViewEngine" Version="1.4.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Fss.Core\Fss.Core.fsproj" />
    </ItemGroup>

</Project>
